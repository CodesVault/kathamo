{
    "name": "codesvalut/kathamo",
    "description": "WordPress plugin starter",
    "type": "project",
    "authors": [
        {
            "name": "CodesVault",
            "email": "codesvault@gmail.com"
        },
        {
            "name": "AbmSourav",
            "email": "keramotul.islam@gmail.com"
        }
    ],
	"autoload": {
        "psr-4": {
            "Kathamo\\App\\": "app/",
            "Kathamo\\Database\\": "database/"
        }
    },
    "scripts": {
        "phpcs": "phpcs ",
        "phpcbf": "phpcbf ",
		"post-package-install": "phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^2.3",
        "codesvault/wp-seeder": "^1.0",
        "codesvault/kathamo-framework": "^0.0.1"
    },
    "require": {
        "codesvault/howdy-qb": "^1.3"
    },
	"repositories": [
		{
			"type": "path",
			"url": "./kathamo-framework"
		}
	]
}
